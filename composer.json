{
    "name": "dontdrinkandroot/activitypub-orm-bundle",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "description": "Symfony Bundle that provides an ORM implementation for the ActivityPub specification.",
    "keywords": [
        "activitypub",
        "symfony",
        "bundle",
        "federation",
        "server",
        "orm"
    ],
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dontdrinkandroot/activitypub-core-bundle.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\ActivityPubOrmBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dontdrinkandroot\\ActivityPubOrmBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/orm": "^3.2",
        "dontdrinkandroot/activitypub-core-bundle": "^0.1.x-dev",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/uid": "^6.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "liip/test-fixtures-bundle": "^2.7",
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "^6.4",
        "symfony/phpunit-bridge": "^6.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "6.4.*",
            "allow-contrib": true
        },
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
}
